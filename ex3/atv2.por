programa {
  funcao inicio() {
    inteiro num1
    inteiro num2
    escreva("Digite um número:")
    leia(num1)
    escreva("Digite um número:")
    leia(num2)

    inteiro soma = num1 + num2
    escreva(soma, "\n")
    inteiro sub = num1 - num2
    escreva(sub, "\n")
    inteiro multi = num1 * num2
    escreva(multi, "\n")
    inteiro div = num1/num2
    escreva(div, "\n")
  }
}
